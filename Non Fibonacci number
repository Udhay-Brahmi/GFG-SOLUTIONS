// Initial Template for C++

#include <bits/stdc++.h>
using namespace std;

 // } Driver Code Ends


// User function Template for C++

class Solution{
public:
    long long int nonFibonacci(int N){
        // code here
        long long int k=1;int c=0;
        while(c!=N){
            if(ceil(sqrt(5*k*k -4))==floor(sqrt(5*k*k-4))|| ceil(sqrt(5*k*k +4))==floor(sqrt(5*k*k+4)) ){}
            else{c++;}
            k++;
        }
        return (k-1);
    }
};

// { Driver Code Starts.

int main(){
    int t;
    cin>>t;
    while(t--){
        int N;
        cin>>N;
        
        Solution ob;
        cout<<ob.nonFibonacci(N)<<endl;
    }
    return 0;
}  // } Driver Code Ends
