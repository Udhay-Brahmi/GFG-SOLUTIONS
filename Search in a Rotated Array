#include <iostream>
using namespace std;

int main() {
	//code
	int n,a,k;cin>>n;
	while(n--){
	    cin>>a;
	    int arr[a];
	    for(int i=0;i<a;i++){cin>>arr[i];}
	    cin>>k;
	    bool gu=true;
	    for(int i=0;i<a;i++){
	        if(arr[i]==k){cout<<i<<endl;gu=false;break;}
	    }
	    if(gu){cout<<-1<<endl;}
	}
	return 0;
}
