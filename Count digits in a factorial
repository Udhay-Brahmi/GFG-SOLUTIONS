// { Driver Code Starts
#include<bits/stdc++.h>
using namespace std;

 // } Driver Code Ends





class Solution{
public:
    int facDigits(int N){
        //code here
        if(N<0)return 0;
        if(N==1)return 1;
        return floor((double)(N*log10(N/M_E) + (log10(2*M_PI*N)/2.0)))+1;
    }
};


// { Driver Code Starts.
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        
        int n;
        cin>>n;
        Solution ob;
        cout<<ob.facDigits(n)<<"\n";
       
        
    }
    return 0;
}  // } Driver Code Ends
