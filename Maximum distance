// { Driver Code Starts


#include <bits/stdc++.h>
using namespace std;

 // } Driver Code Ends




class Solution {
  public:
    int maxDist(int N) {
        // code here
        double sum=0;
        for(int i=1;i<=N;i++){
            sum+=(double)100/i;
        }
        return sum;
    }
};

// { Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        int N;
        
        cin>>N;

        Solution ob;
        cout << ob.maxDist(N) << endl;
    }
    return 0;
}  // } Driver Code Ends
