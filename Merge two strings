#include <iostream>
using namespace std;

int main() {
	//code
	int n;cin>>n;
	while(n--){
	    string a,b;
	    cin>>a>>b;
	    int i=0,j=0;
	    while(i<a.length() || j<b.length()){
	        if(i<a.length()){cout<<a[i];}
	        if(j<b.length()){cout<<b[j];}
	        i++;j++;
	    }
	    cout<<endl;
	}
	return 0;
}
