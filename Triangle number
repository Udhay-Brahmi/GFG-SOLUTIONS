#include <iostream>
using namespace std;

int main() {
	//code
	int n,a;
	cin>>n;
	while(n--){
	    cin>>a;
	    int sum=0;
	    bool gu=false;
	    for(int i=1;i<=a;i++){
	        sum+=i;
	        if(sum==a){gu=true;break;}
	    }
	    if(gu){cout<<1<<endl;}
	    else{cout<<0<<endl;}
	}
	return 0;
}
