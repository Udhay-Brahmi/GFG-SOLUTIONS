#include <iostream>
using namespace std;

int main() {
	//code
	int n,a,d;
    cin>>n;
    while(n--){
        cin>>a;
        int arr[10000]={0},count=0;
        for(int i=0;i<a;i++){
            cin>>d;
            if(arr[d]==0){count++;arr[d]++;}
        }
        cout<<count<<endl;
    }
	return 0;
}
