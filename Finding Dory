// { Driver Code Starts
// Initial Template for C++
#include <bits/stdc++.h>
using namespace std;

 // } Driver Code Ends



// User function Template for C++
class Solution {
  public:
    long long int nisDoryOrNot(long long int n){
        if(ceil(sqrt(5*n+4))==floor(sqrt(5*n+4))|| ceil(sqrt(5*n-4))==floor(sqrt(5*n-4))){
            return 1;
        }
        return 0;
    }
};

// { Driver Code Starts.
int main() {
    int t;
    cin >> t;
    while (t--) {
        long long int n;
        cin >> n;
        Solution ob;
        cout << ob.nisDoryOrNot(n) << endl;
    }
    return 0;
}
  // } Driver Code Ends
