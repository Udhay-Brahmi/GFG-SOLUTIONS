#include <iostream>
using namespace std;

int main() {
	//code
	int n;cin>>n;
	while(n--){
	    int a,b,c,d,ram=0,rohan=0;
	    cin>>a>>b>>c;
	    for(int i=0;i<a;i++){
	        cin>>d;
	        if(d%b==0){ram++;}
	        if(d%c==0){rohan++;}
	    }
	    if(ram>rohan){cout<<"Ram"<<endl;}
	    else if(ram<rohan){cout<<"Rohan"<<endl;}
	    else{
	        cout<<"Both"<<endl;
	    }
	}
	return 0;
}
