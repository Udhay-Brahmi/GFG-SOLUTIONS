#include <bits/stdc++.h>
using namespace std;

int main() {
	//code 
	int n,a;cin>>n;
	while(n--){
	    cin>>a;
	    vector<int> vec;
	    for(int i=0;i<=a;i++){
	        if((i&a)==i){vec.push_back(i);}
	    }
	    reverse(vec.begin() , vec.end());
	    for(auto e:vec)cout<<e<<" ";
	    cout<<endl;
	}
	return 0;
}
